<?xml version="1.0" encoding="UTF-8" ?>
<!-- Definition d'une liste d'etudiants -->
<xs:schema xmlns:xs ="http://www.w3.org/2001/XMLSchema">
  <xs:element name="liste" type="listeEtudiantsType"/>
  <xs:complexType name="listeEtudiantsType">
       <xs:sequence>
          <xs:element name="etudiant" type="xs:string" minOccurs='1' 
                                                       maxOccurs='unbounded'/> 
       </xs:sequence>
  </xs:complexType>
</xs:schema>
